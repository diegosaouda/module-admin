<ul class="token-list list-unstyled">
	<li draggable="true" ng-repeat="item in items" data-id="{{item.id}}" ng-click="itemClicked($index, $event)" ng-class="{selected: item.$selected}" ng-show="item.label">
		<span class="pull-right actions">
			<i class="icon-reorder icon-large" ng-hide="disableReordering" title="{{ i18n('m.rbs.admin.admin.drag_to_reorder', ['ucf']) }}"></i>
			<a ng-href="(= item |Â rbsURL =)" title="{{ i18nAttr('m.rbs.admin.admin.picker_open', ['ucf']) }}"><i class="icon-circle-arrow-right icon-large"></i></a>
			<a ng-hide="readonly" href ng-click="remove($index)"><i class="icon-remove icon-large"></i></a>
		</span>
		<span ng-include="getItemTemplateName(item)"></span>
	</li>
</ul>