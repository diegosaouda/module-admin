    <link href="Rbs/Admin/css/token-list.css" rel="stylesheet"/>
	<link href="Rbs/Admin/menu/menu.css" rel="stylesheet"/>
	<link href="Rbs/Admin/dashboard/dashboard.css" rel="stylesheet"/>

    <!-- modules -->
{% for node in resources.header %}
	{{ node|raw }}
{% endfor %}

	<script type="text/javascript">
		var __change = {};
		__change.menu = {{ resources.menu|json_encode|raw }};
		__change.OAuth = {{ OAuth.Consumer|json_encode|raw }};
	</script>
</head>

<body data-ng-controller="Change.RootController">
    <div full-screen-toggle id="fullScreenBar">
        <i class="icon"></i>Vous êtes en mode plein écran dans RBS Change. Cliquez sur cette barre pour quitter le mode plein-écran.
	</div>

    {% include 'header.twig' %}
    {% include 'menu.twig' %}

    <div class="container-fluid" id="mainWrapper">

        <div class="row-fluid clearfix" id="mainToolbar">
            <div class="btn-toolbar">
                <button type="button" loading-indicator-button="" class="btn btn-large"></button>
                <clipboard-widget></clipboard-widget>
                <button full-screen-toggle type="button" class="btn btn-large" title="Basculer en mode plein écran"></button>
            </div>
            <breadcrumb></breadcrumb>
        </div>

        <div class="row-fluid">
            <div class="span3" id="leftContainer" ng-swipe-right="menu.open()">
                <div id="mainMenu"></div>
            </div>
            <div class="span9" id="mainContainer">
                <notification-center></notification-center>
                <div id="helpListTop"></div>
                <div id="workspace" class="ng-view"></div>
                <div id="helpListBottom"></div>
            </div>
        </div>
    </div>

    <footer class="container-fluid">
        <div class="row-fluid">
			<span class="span3"></span>
			<span class="span9">
				<a onclick="window.open(this.href); return false;" href="http://www.rbschange.com">RBS Change</a> est un CMS e-commerce open-source edité par la société <a onclick="window.open(this.href); return false" href="http://www.rbs.fr">Ready Business System</a>.
			</span>
        </div>
    </footer>

    <script type="text/javascript" src="Rbs/Admin/lib/script.min.js"></script>

